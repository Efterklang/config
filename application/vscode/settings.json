    "vim.normalModeKeyBindings": [
        // view navigation
        {
            "before": [ ":" ],
            "commands": [ "workbench.action.showCommands" ],
            "silent": true
        },
        {
            "before": [ "/" ],
            "commands": [ "actions.find" ]
        },
        // spilit horizontally
        {
            "before": [ "<leader>", "-" ],
            "commands": [ "workbench.action.splitEditorDown" ]
        },
        // spilit vertically
        {
            "before": [ "<leader>", "|" ],
            "commands": [ "workbench.action.splitEditorRight" ]
        },
        {
            "before": [ "<leader>", "m" ],
            "commands": [ "bookmarks.toggle" ]
        },
        {
            "before": [ "<leader>", "b" ],
            "commands": [ "bookmarks.list" ]
        },
        {
            "before": [ "enter" ],
            "commands": [ "lineBreakInsert" ]
        },
        {
            "before": [ "<leader>", "g", "g" ],
            "commands": [ "lazygit-vscode.toggle" ]
        },
        {
            "before": [ "<leader>", "g", "c" ],
            "commands": [ "git-graph.view" ]
        },
        {
            "before": [ "<leader>", "x" ], // focus extensions
            "commands": [ "workbench.view.extensions" ]
        },
        {
            "before": [ "<leader>", "e" ], // focus explorer, cmd-e to hide
            "commands": [ "workbench.view.explorer" ]
        },
        {
            "before": [ "K" ],
            "commands": [ "editor.action.showHover" ]
        },
        // 转到定义
        {
            "before": [ "g", "d" ],
            "commands": [ "editor.action.revealDefinition" ]
        },
        // 转到引用
        {
            "before": [ "g", "r" ],
            "commands": [ "editor.action.goToReferences" ]
        },
        // 转到实现
        {
            "before": [ "g", "i" ],
            "commands": [ "editor.action.goToImplementation" ]
        },
        // 回退到上一个位置
        {
            "before": [ "g", "b" ],
            "commands": [ "workbench.action.navigateBack" ]
        },
        {
            "before": [ "<leader>", "E" ], // focus secondary sidebar, cmd-k + cmd-e to hide
            "commands": [ "workbench.action.focusAuxiliaryBar" ]
        },
        // edit settings.json
        {
            "before": [ "<leader>", "s", "s" ],
            "commands": [ "workbench.action.openSettingsJson" ]
        },
        // open command palette
        {
            "before": [ "<leader>", "p" ],
            "commands": [ "workbench.action.showCommands" ]
        },
        // go to opened tab
        {
            "before": [ "<leader>", "f", "o" ],
            "commands": [ "workbench.action.showAllEditors" ]
        },
        // 全局搜索文件名
        {
            "before": [ "<leader>", "f", "f" ],
            "commands": [ "workbench.action.quickOpen" ]
        },
        // 全局搜索文字
        {
            "before": [ "<leader>", "f", "w" ],
            "commands": [ "workbench.action.quickTextSearch" ]
        },
        // search symbol(current)
        {
            "before": [ "<leader>", "f", "s" ],
            "commands": [ "workbench.action.gotoSymbol" ]
        },
        // search symbol(global)
        {
            "before": [ "<leader>", "f", "S" ],
            "commands": [ "workbench.action.showAllSymbols" ]
        },
        // 退出 vscode
        {
            "before": [ "<leader>", "q" ],
            "commands": [ "workbench.action.closeWindow" ]
        },
        {
            "before": [ "<leader>", "t" ], // focus terminal
            "commands": [ "workbench.action.terminal.toggleTerminal" ]
        },
        {
            "before": [ "<leader>", "d" ], // focus debug
            "commands": [ "workbench.debug.action.toggleRepl" ]
        },
        // Editor Operation
        {
            "before": [ "x" ],
            "commands": [ "workbench.action.closeActiveEditor" ]
        },
        {
            "before": [ "X" ],
            "commands": [ "workbench.action.closeAllEditors" ]
        },
        {
            "before": [ "H" ],
            "commands": [ "workbench.action.navigateBack" ]
        },
        {
            "before": [ "L" ],
            "commands": [ "workbench.action.navigateForward" ]
        }, // gh 到行头
        {
            "before": [ "g", "h" ],
            "commands": [ "cursorHome" ]
        },
        // gl 到行尾
        {
            "before": [ "g", "l" ],
            "commands": [ "cursorEnd" ]
        }
    ],
    "vim.visualModeKeyBindings": [
        // format selection
        {
            "before": [ "/" ],
            "commands": [ "actions.findWithSelection" ]
        },
        {
            "before": [ "f" ],
            "commands": [ "editor.action.formatSelection" ]
        },
        // expand seletion
        {
            "before": [ "l" ],
            "commands": [ "editor.action.smartSelect.grow" ]
        },
        // shrink selection
        {
            "before": [ "h" ],
            "commands": [ "editor.action.smartSelect.shrink" ]
        },
        // stage selection
        {
            "before": [ "s" ],
            "commands": [ "git.stageSelectedRanges" ]
        },
        // revert selection
        {
            "before": [ "r" ],
            "commands": [ "git.revertSelectedRanges" ]
        },
        // Bind > and < in visual mode to indent/outdent lines (repeatable):
        {
            "before": [ ">" ],
            "commands": [ "editor.action.indentLines" ]
        },
        {
            "before": [ "<" ],
            "commands": [ "editor.action.outdentLines" ]
        }
    ],
